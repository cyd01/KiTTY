.PHONY: clean 9bis

clean:
	rm -f *.o *.exe *.res.o *.so *.map

cross:
	sed -i 's#^typedef enum _WTS_VIRTUAL_CLASS #// typedef enum _WTS_VIRTUAL_CLASS #' window.c
	make -e TOOLPATH=/usr/bin/i686-w64-mingw32- -f MAKEFILE.MINGW putty.exe plink.exe pscp.exe psftp.exe pageant.exe puttygen.exe
	make -e TOOLPATH=/usr/bin/i686-w64-mingw32- -f MAKEFILE.MINGW portable
	cp putty.exe putty_nocompress.exe
	upx --best --compress-icons=2 --compress-exports=0 putty.exe putty_portable.exe putty_nocompress.exe
	mv putty.exe /build/kitty.exe
	mv putty_portable.exe /build/kitty_portable.exe
	mv putty_nocompress.exe /build/kitty_nocompress.exe
	mv plink.exe /build/klink.exe
	mv pscp.exe /build/kscp.exe
	mv psftp.exe /build/ksftp.exe
	mv pageant.exe /build/kageant.exe
	mv puttygen.exe /build/kittygen.exe
	rm *.o

cross64:
	sed -i 's#^typedef enum _WTS_VIRTUAL_CLASS #// typedef enum _WTS_VIRTUAL_CLASS #' window.c
	make -e TOOLPATH=/usr/bin/x86_64-w64-mingw32- -f MAKEFILE.MINGW putty.exe

9bis: putty.exe putty_portable.exe plink.exe pscp.exe psftp.exe pageant.exe puttygen.exe

